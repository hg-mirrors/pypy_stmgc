
(defun create-tree (n)
   (if (== n 0) (list 1) (list (create-tree (- n 1)) (create-tree (- n 1))))
)

(defun walk-tree (tree)
   (if (== (len tree) 1) (get tree 0)
      (+ (walk-tree (get tree 0)) (walk-tree (get tree 1)))
   )
)

(defun lookup-tree ()
   (walk-tree (create-tree 10))
)

(setq n 0)
(while (< n 1000)
   (transaction lookup-tree)
   (setq n (+ n 1)))
