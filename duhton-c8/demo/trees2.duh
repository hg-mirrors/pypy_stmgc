

(defun create-tree (n)
   (if (== n 0) 1 (cons (create-tree (- n 1)) (create-tree (- n 1))))
)

(defun walk-tree (tree)
   (if (pair? tree)
      (+ (walk-tree (car tree)) (walk-tree (cdr tree)))
	  1
   )
)



(setq n 0)
(setq tree (create-tree 20))
(print (quote tree-created))
(while (< n 100)
   (transaction walk-tree tree)
   (setq n (+ n 1)))
